{
  "name": "sail-idp",
  "private": true,
  "version": "0.4.2.5",
  "description": "SAIL - Intelligent Document Processing for Stewart Title",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node server.js",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "build": "vite build",
    "production": "npm run build && NODE_ENV=production node server.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "docker:build": "docker build -t sail-idp .",
    "docker:run": "docker run -p 3005:3005 -v sail-idp-data:/app/data sail-idp",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@tanstack/react-query": "^5.90.20",
    "better-sqlite3": "^11.9.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "lucide-react": "^0.563.0",
    "mermaid": "^11.12.2",
    "multer": "^2.0.2",
    "pdfjs-dist": "^5.4.530",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.4.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tailwindcss": "^4.1.18",
    "vite": "^7.2.4",
    "xlsx": "^0.18.5"
  }
}
